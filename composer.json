{
    "name": "byjoby/digraph-core",
    "description": "Source and API version of a CMS that emphasizes flexibility and content permanence. Generally this package is only used for development.",
    "type": "project",
    "license": "MIT",
    "authors": [{
        "name": "Joby Elliott",
        "email": "joby@byjoby.com"
    }],
    "require": {
        "php": ">=7.0",
        "twig/twig": "^2.5",
        "phpmailer/phpmailer": "^6.0",
        "byjoby/destructr": "dev-master",
        "byjoby/formward": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Digraph\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digraph\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    }
}