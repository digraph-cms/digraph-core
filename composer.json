{
    "name": "byjoby/digraph-core",
    "description": "Source and API version of a CMS that emphasizes flexibility and content permanence. Generally this package is only used for development.",
    "type": "project",
    "license": "MIT",
    "authors": [{
        "name": "Joby Elliott",
        "email": "joby@byjoby.com"
    }],
    "require": {
        "php": ">=7.1",
        "twig/twig": "^2.0",
        "phpmailer/phpmailer": "^6.0",
        "byjoby/destructr": "^1",
        "byjoby/formward": "dev-master",
        "byjoby/html-object-strings": "dev-master",
        "symfony/cache": "^4.0",
        "byjoby/sesh": "^0.1.0",
        "erusev/parsedown": "^1.7",
        "imagine/imagine": "^1.1",
        "cogpowered/finediff": "^0.3.1",
        "league/html-to-markdown": "^4.8",
        "lincanbin/white-html-filter": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "monolog/monolog": "^1.24"
    },
    "suggest": {
        "monolog/monolog": "Allows fully integrated logging via the monolog helper"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Digraph\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digraph\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    }
}
