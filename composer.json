{
    "name": "byjoby/digraph-core",
    "description": "PHP CMS with an emphasis on flexibility and URL permanence.",
    "type": "project",
    "license": "MIT",
    "authors": [{
        "name": "Joby Elliott",
        "email": "joby@byjoby.com"
    }],
    "require": {
        "php": ">=7.0",
        "twig/twig": "^2.5",
        "phpmailer/phpmailer": "^6.0",
        "byjoby/flatrr": "^0.1.0",
        "byjoby/destructr": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Digraph\\CMS\\": "digraph/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digraph\\CMS\\": "digraph/tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    },
    "config": {
        "vendor-dir": "digraph/lib/",
        "preferred-install": "dist"
    }
}