{
    "name": "byjoby/digraph-core",
    "description": "Source and API version of a CMS that emphasizes flexibility and content permanence. Generally this package is only used for development.",
    "type": "project",
    "license": "MIT",
    "authors": [{
        "name": "Joby Elliott",
        "email": "joby@byjoby.com"
    }],
    "require": {
        "php": ">=7.1",
        "twig/twig": "^2.0",
        "byjoby/destructr": "^1",
        "byjoby/formward": "dev-master",
        "byjoby/html-object-strings": "dev-master",
        "symfony/cache": "^4.0",
        "byjoby/sesh": "^0.1.0",
        "erusev/parsedown": "^1.7",
        "imagine/imagine": "^1.1",
        "cogpowered/finediff": "^0.3.1",
        "lincanbin/white-html-filter": "^1.3",
        "scrivo/highlight.php": "^9.14",
        "css-crush/css-crush": "^2.4",
        "teamtnt/tntsearch": "^2.0",
        "soundasleep/html2text": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "monolog/monolog": "^1.24",
        "byjoby/digraph-pdf": "dev-master"
    },
    "suggest": {
        "monolog/monolog": "Required for more advanced/reliable logging"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Digraph\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digraph\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    }
}
