{
    "paths": {
        "web": "${paths.base}/web",
        "storage": "${paths.base}/storage"
    },
    "urls": {
        "protocol": null,
        "home_name": "Home",
        "site_path": "/",
        "site_host": null
    },
    "files": {
        "path": "${paths.web}/files",
        "url": "/files",
        "ttl": 3600,
        "css": {
            "minify": true,
            "sourcemap": true,
            "keepimportantcomments": true
        },
        "js": {
            "minify": false
        }
    },
    "images": {
        "ttl": 31536000
    },
    "filestore": {
        "path": "${paths.storage}/filestore",
        "symlink": false
    },
    "embedding": {
        "max-height": 75
    },
    "cookies": {
        "secure": false,
        "duration": "3 months",
        "consent_prompt": false
    },
    "session": {
        "strict_ip_check": true
    },
    "ui": {
        "breadcrumb": {
            "min_length": 1,
            "include_current": true
        }
    },
    "templates": {
        "default": {
            "default": "default.php",
            "error": "minimal.php",
            "500": "minimal.php"
        }
    },
    "fields": {
        "site": {
            "name": "Unnamed site"
        }
    },
    "fs": {
        "links": false,
        "umask_file": 79,
        "umask_dir": 2
    },
    "errors": {
        "display_trace": false,
        "display_trace_to_admins": true
    }
}