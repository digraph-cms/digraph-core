<!DOCTYPE html>
<html lang="en" dir="ltr">

  <head>
    <title>{% include 'digraph/title.twig' %}</title>
    {% include 'digraph/head.twig' %}
  </head>

  <body>
    <div id="page-wrapper">
      {% for area in helper.areas %}
        <div class="digraph-area" id="digraph-{{area}}">
          <div class="digraph-area-wrapper">
            {% include 'areas/' ~ area ~ '.twig' %}
          </div>
        </div>
      {% endfor %}
    </div>

    <script src="{{url.base}}digraph-foot.js"></script>
  </body>

</html>
